function minhaFuncao() {
    console.log("Esta é uma função nomeada.");
}
minhaFuncao();

function soma(a, b) {
    return a + b;
}
console.log(soma(3, 5)); // Saída: 8

var MinhaFuncao = new Function("x", "y", "return x * y;");
console.log(MinhaFuncao(3, 4)); // Saída: 12

var minhaFuncao = function() {
    console.log("Esta é uma função literal.");
};
minhaFuncao();

var dobrar = (num) => num * 2;
console.log(dobrar(5)); // Saída: 10
